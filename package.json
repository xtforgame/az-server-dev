{
  "name": "az-server-dev",
  "version": "1.0.0",
  "description": "A server-side seed project",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "gulp",
    "seed",
    "boilerplate",
    "kickstarter",
    "ES6",
    "6to5"
  ],
  "author": "Rick Chen <xtforgame@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --fix --ext .js ./src",
    "lint-all": "eslint --ext .js ./src ./test ./internals",
    "lint-all-fix": "eslint --fix --ext .js ./src ./test ./internals",
    "build": "cross-env NODE_ENV=production gulp server:build && cross-env NODE_ENV=production gulp server:build:extras",
    "start": "cross-env NODE_ENV=development gulp server:watch:dev",
    "debug": "cross-env NODE_ENV=development gulp server:watch:dev --inspect",
    "test": "eslint --ext .js ./test/server && cross-env NODE_ENV=test mocha --require babel-core/register './test/server/**/*.spec.js'",
    "test-watch": "eslint --ext .js ./test/server && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js --require babel-core/register './test/server/**/*.spec.js'"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "app-root-path": "^1.0.0",
    "az-restful-helpers": "git+https://github.com/xtforgame/az-restful-helpers.git#v0.1.0",
    "azldi": "git+https://github.com/xtforgame/azldi.git#v0.1.0",
    "generic-router": "git+https://github.com/xtforgame/generic-router#v1.2.0",
    "http-shutdown": "^1.2.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0"
  },
  "devDependencies": {
    "az-gulp-env-lite": "git+https://github.com/xtforgame/az-gulp-env-lite#v1.0.0-alpha.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.5.4",
    "sinon": "^4.3.0"
  },
  "false": {}
}
